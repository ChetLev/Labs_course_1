#include <iostream>
#include <set>

int main() {
    int n;
    std::cout << "Введите количество элементов: ";
    std::cin >> n;

    std::set<int> unique;

    std::cout << "Введите элементы массива: ";
    for (int i = 0; i < n; ++i) {
        int element;
        std::cin >> element;
        unique.insert(element); // добавл элемент в множество
    }

    std::cout << "Уникальные элементы: ";
    for (const int& elem : unique) {
        std::cout << elem << " ";
    }
    std::cout << std::endl;

    return 0;
}
